{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "curatenieperfecta": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/curatenieperfecta",
           
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/images",
              "src/manifest.webmanifest"
            ],
            "styles": [
              
              "src/assets/libraries/bootstrap/bootstrap.min.css",
              "src/assets/libraries/fuelux/jquery-ui.min.css",
              "src/assets/libraries/owl-carousel/owl.carousel.css", 
              "src/assets/libraries/owl-carousel/owl.theme.css", 
              "src/assets/libraries/fonts/font-awesome.min.css",
              "src/assets/libraries/animate/animate.min.css",
              "src/assets/libraries/flexslider/flexslider.css" , 
              "src/assets/libraries/magnific-popup.css" ,
              "src/assets/css/components.css",
              "src/assets/style.css",
              "src/assets/css/media.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/libraries/jquery.min.js",	
              "src/assets/libraries/jquery.easing.min.js",
              "src/assets/libraries/bootstrap/bootstrap.min.js",
              "src/assets/smtp.js",	
              "src/assets/libraries/gmap/jquery.gmap.min.js",
              "src/assets/libraries/fuelux/jquery-ui.min.js",
              "src/assets/libraries/jquery.animateNumber.min.js",
              "src/assets/libraries/jquery.appear.js",
              "src/assets/libraries/jquery.knob.js",
              "src/assets/libraries/wow.min.js",
              "src/assets/libraries/owl-carousel/owl.carousel.min.js",
              "src/assets/libraries/portfolio-filter/jquery.quicksand.js",
              "src/assets/libraries/expanding-search/modernizr.custom.js",
              "src/assets/libraries/flexslider/jquery.flexslider-min.js",
              "src/assets/libraries/jquery.magnific-popup.min.js",
              "src/assets/libraries/expanding-search/modernizr.custom.js",
              "src/assets/libraries/expanding-search/classie.js",
              "src/assets/libraries/expanding-search/uisearch.js",
              "src/assets/js/functions.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "curatenieperfecta:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "curatenieperfecta:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "curatenieperfecta:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "curatenieperfecta:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "curatenieperfecta:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "curatenieperfecta"
}
